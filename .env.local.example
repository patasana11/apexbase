# Example local development environment configuration
# Copy this file to .env.local and update the values

# Azure OpenAI Configuration
NEXT_PUBLIC_AZURE_OPENAI_API_KEY=your_openai_api_key_here
NEXT_PUBLIC_AZURE_OPENAI_ENDPOINT=your_openai_endpoint_here
NEXT_PUBLIC_AZURE_OPENAI_DEPLOYMENT_ID=gpt-4

# GSB Base Configuration
NEXT_PUBLIC_GSB_BASE_DOMAIN=gsbapps.net
NEXT_PUBLIC_DEFAULT_TENANT_CODE=apexbase
NEXT_PUBLIC_GSB_COMMON_TENANT=common

# Authentication Configuration
# Set to true to enable multi-tenant support
NEXT_PUBLIC_ENABLE_MULTI_TENANT=true

# Social Authentication Keys (Only needed for social login testing)
NEXT_PUBLIC_GOOGLE_CLIENT_ID=
NEXT_PUBLIC_FACEBOOK_APP_ID=
NEXT_PUBLIC_APPLE_CLIENT_ID= 